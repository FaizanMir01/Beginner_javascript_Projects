<%- include('header.ejs') %>
    <% posts.forEach(post => { %>
        <div class="card-container">
        <div class="card">
            <h1><%= post.name %></h1>
            <img src="<%= post.image %>" alt="">
            <p>Age:<%= post.age %></p>
            <form action="/view/<%= post._id %>" method="get">
                <button>View Post</button>
            </form>
        </div>
    </div>
    <% }) %>
        
